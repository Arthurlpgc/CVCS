datasource db {
  provider = "mysql"
  url      = "mysql://root:vitaes@mysql:3306/vitaes"
}

generator prisma_client {
  provider = "prisma-client-js"
}
     
model User {
  vid       Int      @id
  autosave  Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
